<h1>Add President</h1>
<%= form_for :president, url: admin_president_add_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
	
  <p>
    <%= f.label :party %><br>
    <%= f.select(:party, [["Republican", "Republican"], ["Democratic", "Democratic"]] ) %>
  </p>
 
  <p>
    <%= f.label :wiki %><br>
    <%= f.text_area :wiki %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
<td><%= link_to 'Back', admin_home_path%></td>
