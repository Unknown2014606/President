<h1>Edit Video</h1>
<%= form_for :video, url: admin_video_update_path(@video), method: :patch do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
	
  <p>
    <%= f.label :topic %><br>
    <%= f.text_field :topic %>
  </p>
 
  <p>
    <%= f.label :president %><br>
    <%= f.text_area :president %>
  </p>
	
  <p>
    <%= f.label :embed %><br>
    <%= f.text_area :embed %>
  </p>
	
  <p>
    <%= f.label :pic %><br>
    <%= f.text_area :pic %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
<td><%= link_to 'Back', admin_video_list_path%></td>