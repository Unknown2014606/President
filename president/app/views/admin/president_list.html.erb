<html>

<head>
	<meta name="Unknown Team" http-equiv="Content-Type" content="text/html; charset=gb2312" />
 	<%= stylesheet_link_tag "admin_pre_list" %>
</head>


<body>

<div class="container">
	<div class="jumbotron">
		<h1>Presidents List</h1>
	</div>


	<table class="table">
  		
 		<thread>
			<tr>
	            <th>Name</th>
	            <th>Party</th>
	            <th>Wiki</th>
	        </tr> 		
 		
  			<% @presidents.each do |president| %>
    		<tr>
      		<td><%= president.name %></td>
      		<td><%= president.party %></td>
			<td><%= president.wiki %></td>
			<td><%= link_to 'Show', admin_president_path(president)%></td>
			<td><%= link_to 'Edit', admin_president_edit_path(president)%></td>
			<td><%= link_to 'Destroy', admin_president_delete_path(president),
			                    method: :delete %></td>
    		</tr>
  			<% end %>
  		</thread>
	</table>


	<div class="label well">
	
		<%= link_to 'Back', admin_home_path%>
	</div>
</div>
</body>
</html>
